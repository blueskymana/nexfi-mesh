
config wifi-device 'radio0'
	option type 'mac80211'
	option channel '36'
	option hwmode '11a'
	option country 'CN'
	option path 'pci0000:00/0000:00:00.0'
	option htmode 'VHT80'
	option disabled '0'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option network 'nexfinet'
	option mode 'adhoc'
	option bssid '66:61:00:11:22:44'
	option ssid 'nexfi-ad'
	option encryption 'none'
    option key '496390182735'

config wifi-device 'radio1'
	option type 'mac80211'
	option channel '11'
	option hwmode '11ng'
	option country 'CN'
	option path 'platform/qca953x_wmac'
	option htmode 'HT40'
	option disabled '0'

config wifi-iface 'default_radio1'
	option device 'radio1'
	option network 'lan'
	option mode 'ap'
	option ssid 'j0'
	option encryption 'psk-mixed'
    option key '12345678'

