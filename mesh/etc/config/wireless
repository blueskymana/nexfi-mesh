# netpool protocol
config wifi-iface 'default_radio1'
	option device 'radio1'
	option mode 'adhoc'
	option ssid 'nexfi-ad'
	option bssid '00:0B:27:E8:E4:3D'
	option encryption 'none'
	option network 'batnet'

